{
  "name": "mei-analyser",
  "private": true,
  "version": "0.1.6-dev",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "docs": "jsdoc --destination ./dist/docs --readme README.md -r src",
    "database": "mkdir ./dist/database && cp ./GABCtoMEI/MEI_outfiles/*.mei ./dist/database",
    "build": "vite build --base=/mei-analyser/ && npm run docs && npm run database",
    "preview": "vite preview --base=/mei-analyser/",
    "updateDatabase": "node src/search/updateDatabase.cjs",
    "test": "node src/analysis/test-algorithm.js"
  },
  "devDependencies": {
    "jsdoc": "^4.0.2",
    "vite": "^5.0.8"
  }
}
